        <jsScript_script>var base = getVariable("API_BASE","https://archive-api.open-meteo.com/v1/archive");
var tz   = getVariable("TIMEZONE","Europe/Lisbon");

var _lat = (lat == null) ? getVariable("LAT_DEFAULT","41.545") : lat;
var _lon = (lon == null) ? getVariable("LON_DEFAULT","-8.426") : lon;

var d = data_yyyy_mm_dd;
if (d instanceof Date) {
  var yyyy=d.getFullYear(), mm=('0'+(d.getMonth()+1)).slice(-2), dd=('0'+d.getDate()).slice(-2);
  d = yyyy+'-'+mm+'-'+dd;
} else { d = String(d); }

var url = base
  + "?latitude="+_lat
  + "&amp;longitude="+_lon
  + "&amp;daily=weathercode,precipitation_sum"
  + "&amp;timezone="+encodeURIComponent(tz)
  + "&amp;start_date="+d
  + "&amp;end_date="+d;
</jsScript_script>
      </jsScript>
