    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Converte o valor vindo do JSON para número inteiro.
// (O JSON input por vezes entrega string; parseInt evita falhas no indexOf)
var wc = parseInt(weathercode, 10);

// Listas de códigos WMO (Open-Meteo usa estes códigos)
// Agrupamos por categorias para ficar fácil de manter
var claro     = [0, 1];                   // Céu limpo / maioritariamente limpo
var poucoNuv  = [2];                      // Poucas nuvens
var nublado   = [3];                      // Nublado
var nevoeiro  = [45, 48];                 // Nevoeiro / Nevoeiro geada
var chuvisco  = [51, 53, 55];             // Chuvisco (drizzle) fraco/médio/forte
var chuva     = [61, 63, 65, 80, 81, 82]; // Chuva contínua + aguaceiros
var geloChuva = [66, 67];                 // Chuva gelada (freezing rain)
var neve      = [71, 73, 75, 85, 86];     // Neve / aguaceiros de neve
var trovoada  = [95, 96, 99];             // Trovoada (com/sem granizo)

// Valor por omissão (caso apareça um código não mapeado)
var condicao = "desconhecido";

// Regras de classificação por prioridade
if (claro.indexOf(wc) >= 0 || poucoNuv.indexOf(wc) >= 0) {
  condicao = "sol";
} else if (nublado.indexOf(wc) >= 0) {
  condicao = "nublado";
} else if (nevoeiro.indexOf(wc) >= 0) {
  condicao = "nevoeiro";
} else if (chuvisco.indexOf(wc) >= 0) {
  condicao = "chuvisco";
} else if (chuva.indexOf(wc) >= 0) {
  condicao = "chuva";
} else if (geloChuva.indexOf(wc) >= 0) {
  condicao = "chuva gelada";
} else if (neve.indexOf(wc) >= 0) {
  condicao = "neve";
} else if (trovoada.indexOf(wc) >= 0) {
  condicao = "trovoada";
}

// Refinamento: se o código estiver no intervalo "céu" (0–3)
// e não houver precipitação, força "sol"
if ((wc >= 0 &amp;&amp; wc &lt;= 3) &amp;&amp; (precipitation_sum == null || Number(precipitation_sum) === 0)) {
  condicao = "sol";
}

// Exporta o campo final para a stream
var condicao_meteorologica = condicao;



</jsScript_script>
      </jsScript>
